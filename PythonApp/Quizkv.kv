#:kivy 1.9.1
<Test_quiz>:
    level_box: lvl_box.__self__
    box: box
    BoxLayout:
        id: box
        orientation:'vertical'
        ActionBar:
            background_image: ''
            background_color: 135/255,206/255,235/255
            size_y: '30sp'
            ActionView:
                ActionPrevious:
                    title: '[b]Back[/b]' 
                    app_icon: ''
                    color: 0,0,0,1
                    previous_image_width: self.height
                    with_previous: True
                    markup: True
                    on_release: root.ScreenBack()
        BoxLayout:
            id: lvl_box
            orientation:'vertical'
            padding: ['20sp', '20sp']
            Button:
                background_normal:'normal.png'
                background_down:'down.png'
                text_size: self.width,None
                height: self.texture_size[1]
                size_hint_y: 0.25
                text:'To improve your Japanese language, remember at least 10 words every day.'
                color: (0,0,0,1)
                font_size: '18sp'
            BoxLayout:
                orientation:'vertical'
                spacing: '3sp'
                padding: ['2sp','1sp','4sp','7sp']
                size_hint_y: 0.55
                canvas.before:
                    Color:
                        rgb: (214/255, 111/255, 115/255)
                    Rectangle:
                        size: self.size
                        pos:  self.pos
                Button:
                    background_normal:'normal.png'
                    background_color: (0,0,205/255,0.4)
                    text:'[b]Level Test[/b]\nAll Words'
                    font_size: self.height/3
                    markup: True
                    on_press: root.quizscreen('1')
                Button:
                    background_normal:'normal.png'
                    background_color: (0,102/255,0,0.4)
                    text:'[b]Kanji Test[/b]\nAll Words'
                    font_size: self.height/3
                    markup: True
                    on_press: root.quizscreen('2')
                Button:
                    background_normal:'normal.png'
                    background_color: (128/255,0,128/255,0.4)
                    text:'[b]All Grammars\n     Test[/b]'
                    on_press: root.quizscreen('3')
                    font_size: self.height/3
                    markup: True
            Button:
                text:'Check a level every day at least 10 Questions.'
                color: 0,0,0,1
                font_size: '18sp'
                background_down:'down.png'
                background_normal:'normal.png'
                text_size: self.width,None
                height: self.texture_size[1]
                size_hint_y: 0.2          

<ChallengeScreen>:
    scroll: scroll.__self__
    quiz_box: quiz_box.__self__
    box:box
    grid:grid
    grid4:grid4
    grid1:grid1
    grid2:grid2
    bttn1: bttn1
    bttn2: bttn2
    bttn3: bttn3
    bttn4: bttn4
    BoxLayout:
        id: box
        orientation:'vertical'
        ActionBar:
            background_image: ''
            background_color: 135/255,206/255,235/255
            size_y: '30sp'
            ActionView:
                ActionPrevious:
                    title: '[b]Back[/b]' 
                    app_icon: ''
                    color: 0,0,0,1
                    previous_image_width: self.height
                    with_previous: True
                    markup: True
                    on_release: root.back()

        ScrollView:
            id: scroll
            do_scroll_y: True
            size_hint:(1,1)
            BoxLayout:
                padding: ['25sp','8sp']
                orientation:'vertical'
                height: self.minimum_height
                size_hint:(1,None)
                spacing:'5sp'             
                Label:
                    color: 0,0,0,1
                    size_hint_y: None
                    text:'Simple Quiz'
                    text_size: self.width,None
                    height: self.texture_size[1]
                    font_size: '17sp'
                GridLayout:
                    id:grid
                    rows:4
                    spacing: '3sp'
                    padding: ['1sp','1sp','3sp','4sp']
                    height: self.minimum_height
                    size_hint_y: None
                    canvas.before:
                        Color:
                            rgb: (145/255, 146/255, 230/255)
                        Rectangle:
                            size: self.size
                            pos:  self.pos
                Label:
                    color: 0,0,0,1
                    size_hint_y: None
                    text:'Normal Quiz\n Do zero mistakes.'
                    text_size: self.width,None
                    font_size: '17sp'
                    height: self.texture_size[1]
                GridLayout:
                    id:grid4
                    rows:4
                    spacing: '3sp'
                    padding: ['1sp',0,'3sp','4sp']
                    height: self.minimum_height
                    size_hint_y: None
                    canvas.before:
                        Color:
                            rgb: (233/255, 158/255, 165/255)
                        Rectangle:
                            size: self.size
                            pos:  self.pos
                Label:
                    color: 0,0,0,1
                    size_hint_y: None
                    text:'Hard Quiz\n If you mistake, the question will be changed for the next try.'
                    text_size: self.width,None
                    font_size: '17sp'
                    height: self.texture_size[1]
                GridLayout:
                    id:grid1
                    rows:4
                    height: self.minimum_height
                    spacing: '3sp'
                    padding: ['1sp',0,'3sp','4sp']
                    size_hint_y: None
                    canvas.before:
                        Color:
                            rgb: (210/255,105/255,30/255,0.7)
                        Rectangle:
                            size: self.size
                            pos:  self.pos

                Label:
                    padding:['5sp', '1sp']
                    color: 0,0,0,1
                    size_hint_y: None
                    text:'If you mistake, the quiz game will restart automatically.'
                    text_size: self.width,None
                    font_size: '17sp'
                    height: self.texture_size[1]
                GridLayout:
                    id:grid2
                    rows:5
                    spacing: '3sp'
                    height: self.minimum_height
                    padding: ['1sp',0,'3sp','4sp']
                    size_hint_y: None
                    canvas.before:
                        Color:
                            rgb: (1,105/255,180/255,0.7)
                        Rectangle:
                            size: self.size
                            pos:  self.pos                  
                Label:
                    size_hint_y: None
                    color: 0,0,0,1
                    font_size: '17sp'
                    text:"From Level 9 To Level 14, questions will change every time. Try daily after Unlock."
                    text_size: self.width,None
                    height: self.texture_size[1]
                    
        BoxLayout:
            id: quiz_box
            spacing: '3sp'
            orientation:'vertical'
            padding: ['5sp','3sp','5sp',0]
            size: root.size
            canvas.before:
                Rectangle:
                    size: self.size
                    pos:  self.pos
                    source: 'bg1.jpg'
            Button:
                text:'[b]'+root.error+'[/b]'
                background_down: 'down.png'
                background_normal: 'normal.png'
                text_size: self.width, None
                height: self.texture_size[1]
                background_color: 0,153/255,0
                markup: True
                size_hint_y: 0.1
                font_size: '19sp'
                halign: 'center'      
            GridLayout:
                cols:2
                size_hint_y: 0.08
                padding: '5sp'
                canvas.before:
                    Color:
                        rgb: (245/255,245/255,245/255)
                    Rectangle:
                        size: self.size
                        pos:  self.pos

                Label:
                    text:str(root.page_number)+' Page'
                    color: 0,0,0,1
                    size_hint_x: 0.5
                Label:
                    text: str(root.error_number) + ' Wrong'
                    color: 0,0,0,1
                    size_hint_x: 0.5

            Label:
                size_hint_y: 0.39
                text: '[b]'+root.question+'[/b]'
                color: 0,0,0,1
                text_size: self.width, None
                height: self.texture_size[1]
                font_size: str(root.fontSize) + 'sp'
                halign: 'center'
                font_name: 'mona.ttf'
                markup: True

            Button:
                id:bttn1 
                background_normal: 'normal.png'
                background_color:(41/255, 47/255, 180/255)        
                text:root.ans
                text_size: self.width, None
                halign: 'center'
                size_hint_y: 0.1
                font_size: '19sp'
                font_name: 'mona.ttf'
                on_press:
                    root.no=root.no+1
                    root.go_next(bttn1.text)
            Button:
                id:bttn2
                background_normal: 'normal.png'
                background_color:(41/255, 47/255, 180/255)
                text: root.ans1
                text_size: self.width, None
                halign: 'center'
                size_hint_y: 0.1
                font_size: '19sp'
                font_name: 'mona.ttf'
                on_press:
                    root.no=root.no+1
                    root.go_next(bttn2.text)    
                
            Button:
                id:bttn3
                text_size: self.width, None
                halign: 'center'
                background_normal: 'normal.png'
                background_color:(41/255, 47/255, 180/255)
                text:root.ans2
                size_hint_y: 0.1
                font_size: '19sp'
                font_name: 'mona.ttf'
                on_press:
                    root.no=root.no+1
                    root.go_next(bttn3.text)

            Button:
                id:bttn4
                text:root.ans3
                background_normal: 'normal.png'
                background_color:(41/255, 47/255, 180/255)
                text_size: self.width, None
                halign: 'center'
                size_hint_y: 0.1
                font_size: '19sp'
                font_name: 'mona.ttf'
                on_press:
                    root.no=root.no+1
                    root.go_next(bttn4.text)
            Label:
                size_hint_y: 0.03

<word_list>:
    layout:layout
    page_grammar:page_one.__self__
    page_word:page_two.__self__
    pagebox:pagebox
    BoxLayout:
        id:layout
        orientation:'vertical'
        ActionBar:
            background_image: ''
            background_color: 135/255,206/255,235/255
            size_y: '25sp'
            ActionView:
                padding: ['5sp','2sp']
                ActionPrevious:
                    title: '[b]Back[/b]' 
                    app_icon: ''
                    color: 0,0,0,1
                    previous_image_width: self.height
                    with_previous: True
                    markup: True
                    on_release:
                        app.num = 0
                        root.manager.current='home'
                ActionButton:
                    text: '[b]'+root.value+'[/b]'
                    on_release: root.all_list()
                    background_color: (0,153/255,0,0.6)
                    background_normal:'normal.png'
                    font_size: '20sp'
                    markup: True
        BoxLayout:
            id: page_one
            size_hint_y: 0.1
            padding: ['5sp',0]
            spacing: '3sp'
            Label:
                font_size: '17sp'
                text:'Page: [color=#0000ff]'+str(root.pageno1)+'[/color]'
                color : 0,0,0
                markup: True       
            Button:
                text:'1'
                font_size: '18sp'
                background_normal:'normal.png'
                background_color:(1,0,0,0.5)
                color:(0,0,0,1)
                on_press: root.page(1,'nn')
            Button:
                text:'2'
                font_size: '18sp'
                background_normal:'normal.png'
                background_color:(204/255,229/255,1)
                color:(0,0,0,1)
                on_press: root.page(2,'nn')
            Button:
                text:'3'
                font_size: '18sp'
                background_normal:'normal.png'
                background_color:(204/255,229/255,1)
                color:(0,0,0,1)
                on_press: root.page(3,'nn')
        BoxLayout:
            id: page_two
            size_hint:(1,0.1)
            padding: ['5sp', '2sp']
            Label:
                text:'Page: '+str(root.pageno)
                width: '70sp'
                color : 0,0,0
                font_size: '18sp'
                size_hint_x: None
            ScrollView:
                do_scroll_x: True
                do_scroll_y: False
                size_hint: (1,1)           
                BoxLayout:
                    id: pagebox
                    spacing: '2sp'
                    width: self.minimum_width
                    size_hint_x: None

        ScrollView:
            do_scroll_y: True
            size_hint: (1,0.92)
            size:(root.width, root.height)
            BoxLayout:
                orientation:'vertical'
                padding: ['8sp','12sp']
                height: self.minimum_height
                size_hint:(1,None)
                canvas.before:
                    Color:
                        rgb: (245/255,245/255,245/255)
                    Rectangle:
                        size: self.size
                        pos:  self.pos
                Label:
                    text_size: self.width, None
                    height: self.texture_size[1]    
                    text:root.text
                    color: 0,0,0,1
                    font_name: 'mona.ttf'
                    font_size: '18sp'
                    size_hint_y:None
                    markup:True

<popup_box>:
    id:pop
    title_size:'18sp'
    size_hint: (1, 0.5)
    pos_hint:{"top":0.98}
    title: root.pop_title
    BoxLayout:
        size_hint:(1,1)
        orientation:'vertical'
        GridLayout:
            cols:1
            size_hint_y:0.8
            Label:
                id:btn
                text_size: self.width, None
                height: self.texture_size[1]    
                text:root.pop_contents
                font_size: '18sp'
                markup: True
                size_hint_y:None
                halign: 'center'
                padding: ['5sp', '5sp']
                font_name: 'mona.ttf'
        Button:
            text:'Close Me'
            size_hint: (1, 0.2)
            on_press: pop.dismiss()
        
<Mystory@Button>:
    color: 0,0,0,1
    font_size: '19sp'
    background_normal: 'normal.png'
    background_color:204/255,229/255,1
    width: '70sp'
    size_hint_x: None
<ReadingScreen>:
    box:box
    img:img
    BoxLayout:
        orientation:'vertical'
        size: root.size
        ActionBar:
            background_image: ''
            background_color: 135/255,206/255,235/255
            size_y: '30sp'
            ActionView:
                ActionPrevious:
                    title: '[b]Back[/b]' 
                    app_icon: ''
                    color: 0,0,0,1
                    previous_image_width: self.height
                    with_previous: True
                    markup: True
                    on_release:
                        app.num = 0
                        root.manager.current='home'
        BoxLayout:
            id: boxx
            size_hint: (1,0.1)
            canvas.before:
                Color:
                    rgb: (192/255,192/255,192/255,0.6)
                Rectangle:
                    size: self.size
                    pos:  self.pos         
            ScrollView:
                do_scroll_x: True
                do_scroll_y: False
                size: boxx.width, boxx.height
                size_hint: (1, 1)  
                BoxLayout:
                    id: box
                    padding: ['5sp', '2sp']
                    spacing: '2sp'
                    size_hint_x: None
                    width: self.minimum_width
                    Mystory:
                        text: 'Easy 1'
                        on_press: 
                            img.source='story0.png'
                            root.popp('story0',0)
                    Mystory:
                        text: 'Story 2'
                        on_press: 
                            img.source='story1.png'
                            root.popp('story1',1)                   
                    Mystory:
                        text: 'Story 3'
                        on_press: 
                            img.source='story2.png'
                            root.popp('story2',2)  
                    Mystory:
                        text: 'Story 4'
                        on_press: 
                            img.source='story3.png'
                            root.popp('story3',3)
                    Mystory:
                        text: 'Story 5'
                        on_press: 
                            img.source='story4.png'
                            root.popp('story4',4)
                    Mystory:
                        text: 'Story 6'
                        on_press: 
                            img.source='story5.png'
                            root.popp('story5', 5)
                    Mystory:
                        text: 'Story 7'
                        on_press: 
                            img.source='story6.png'
                            root.popp('story6',6)
                    Mystory:
                        text: 'Story 8'
                        on_press: 
                            img.source='story7.png'
                            root.popp('story7',7)          
        ScrollView:       
            do_scroll_x: False
            do_scroll_y: True
            size_hint_y:0.8
            size: root.width, root.height
            GridLayout:
                padding: '7sp'
                cols:1
                size_hint_y:None
                height: self.minimum_height
                canvas.before:
                    Color:
                        rgb: (245/255,245/255,245/255)
                    Rectangle:
                        size: self.size
                        pos:  self.pos
                Image:
                    id: img
                    source: 'book.png'
                    keep_ratio: False
                    allow_stretch: True
                    height: '125sp'
                    size_hint_y:None
                    size_hint_x: 1
                Button:
                    background_down: 'down.png'
                    background_normal: 'normal.png'
                    text_size: self.width, None
                    height: self.texture_size[1]    
                    text:root.text
                    font_size: '19sp'
                    size_hint_y:None
                    padding: ['3sp', '8sp']
                    color: (0,0,0,1)
                    font_name:'mona.ttf'
                    markup: True
                    on_ref_press: root.search(args[1])

<QuizScreen>:
    backBtn:button1
    bttn1:bttn1
    bttn2:bttn2
    bttn3:bttn3
    enter: enter
    hint_box: hnt.__self__
    padding: '5sp'
    orientation: 'vertical'
    size: root.size
    GridLayout:
        cols:2
        rows:2
        padding: ['25sp','1sp']
        size_hint_y: 0.13
        canvas.before:
            Color:
                rgb: (245/255,245/255,245/255)
            Rectangle:
                size: self.size
                pos:  self.pos
        TextInput:
            id: enter
            hint_text: "Enter Page"
            multiline: False
            on_text_validate: root.search(enter.text); enter.text=''

        Button:
            background_normal:'normal.png'
            background_color: (0,245/255,245/255)
            color: 0,0,0,1
            font_size: '18sp'
            text:'Search'
            on_release: root.search(enter.text); enter.text=''
        Label:
            color: 0,0,0,1
            text:str(root.page_number)+' Page'
        Label:
            text: str(root.error_number) + ' Wrong'
            color: 0,0,0,1
    Label:
        text: root.error
        size_hint_y: 0.08
        text_size: self.width, None
        height: self.texture_size[1]
        font_size:'18sp'
        markup: True
        color: 0,0,0,1
    Label:
        id: label1         
        size_hint_y: 0.27
        text: root.question
        color: 0,0,0,1
        text_size: self.width, None
        height: self.texture_size[1]
        halign: 'center'
        font_name: 'mona.ttf'
        markup: True
    Label:
        id:hnt
        size_hint_y:0.07
        height: self.texture_size[1]
        font_size: '17sp'
        markup: True
        on_ref_press:
            hnt.text = root.hint
            hnt.color = (0,0.6,0)
    Button:
        id:bttn1         
        text:root.ans
        size_hint_y: 0.1
        font_size: '20sp'
        font_name: 'mona.ttf'
        on_press:
            root.no = root.no + 1
            root.go_next(bttn1.text)
    Button:
        id:bttn2
        text: root.ans1
        size_hint_y: 0.1
        font_size: '20sp'
        font_name: 'mona.ttf'
        on_press:
            root.no = root.no + 1
            root.go_next(bttn2.text)           
    Button:
        id:bttn3
        text:root.ans2
        size_hint_y: 0.1
        font_size: '20sp'
        font_name: 'mona.ttf'
        on_press:
            root.no = root.no + 1
            root.go_next(bttn3.text)
    BoxLayout:
        padding: '5sp'
        size_hint_y: 0.15
        Button:
            id: button1
            text: 'Go Back'
            font_size: '20sp'
            background_color: 1,0,0,1
            on_press:
                root.no = root.no - 1
                root.go_next('4')  # ボタンをクリックした時
        Button:
            id: button2
            text: 'Re-Start'
            font_size: '20sp'
            background_color: 1,0,0,1
            on_press:
                root.no =int(1)
                root.go_next('4')  # ボタンをクリックした時   

<HomeScreen>:  
    padding: '5sp'
    BoxLayout:
        orientation: 'vertical'
        ActionBar:
            background_image: ''
            background_color: 135/255,206/255,235/255
            size_y: '35sp'
